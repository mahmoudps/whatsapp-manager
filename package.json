  "scripts": {
    "prebuild": "node scripts/generate-env.js && node scripts/init-database.js",
    "setup": "node scripts/generate-env.js && node scripts/init-database.js",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "production": "npm run build && npm run build:ws && npm start",
    "lint": "next lint --dir .",
    "lint:fix": "next lint --dir . --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "npm ci && npm test",
    "ws": "ts-node -P tsconfig.ws.json lib/websocket-server.ts",
    "build:ws": "tsc -p tsconfig.ws.json",
    "prews:prod": "npm run build:ws",
    "ws:prod": "node dist/websocket-server.js",
    "prepare": "npm run build:ws",
    "rebuild:native": "npm rebuild bcrypt better-sqlite3 --build-from-source",
    "postinstall": "npm run rebuild:native"
  },
